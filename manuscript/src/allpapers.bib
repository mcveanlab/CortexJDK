@article{Walsh:2003p163,
author = {Walsh, Emily C and Mather, Kristie A and Schaffner, Stephen F and Farwell, Lisa and Daly, Mark J and Patterson, Nick and Cullen, Michael and Carrington, Mary and Bugawan, Teodorica L and Erlich, Henry and Campbell, Jay and Barrett, Jeffrey and Miller, Katie and Thomson, Glenys and Lander, Eric S and Rioux, John D},
title = {{An integrated haplotype map of the human major histocompatibility complex}},
year = {2003},
volume = {73},
number = {3},
pages = {580--590},
month = sep,
affiliation = {Center for Genome Research, Whitehead Institute for Biomedical Research, Cambridge, MA 02139, USA.},
doi = {10.1086/378101},
