@article{Gangnard:2013fu,
author = {Gangnard, St{\'e}phane and Badaut, Cyril and Ramboarina, St{\'e}phanie and Baron, Bruno and Ramdani, Tarik and Gamain, Beno{\^\i}t and Deloron, Philippe and Lewit-Bentley, Anita and Bentley, Graham A},
title = {{Structural and immunological correlations between the variable blocks of the VAR2CSA domain DBL6$\varepsilon$ from two Plasmodium falciparum parasite lines.}},
journal = {Journal of molecular biology},
year = {2013},
volume = {425},
number = {10},
pages = {1697--1711},
month = may,
affiliation = {Unit{\'e} d'Immunologie Structurale, D{\'e}partement de Biologie Structurale et Chimie, Institut Pasteur, 25 rue du Docteur Roux, 75724 Paris, France.},
doi = {10.1016/j.jmb.2013.02.014},
pmid = {23429057},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-07-12T10:06:26GMT},
date-modified = {2016-12-23T22:13:47GMT},
abstract = {Plasmodium falciparum erythrocyte membrane protein 1 (PfEMP1), a family of adhesins of the falciparum species of the malaria parasite, is exposed on the surface of the infected erythrocyte. In general, only one PfEMP1 variant is expressed at a time but switching between variants occurs, changing both host-cell receptor specificity and serotype. The PfEMP1 variant VAR2CSA causes sequestration of infected erythrocytes in the intervillous spaces of the placenta via the glycosaminoglycan chondroitin sulfate A. This leads to pregnancy-associated malaria, which has severe consequences for the fetus and mother. The extracellular region of VAR2CSA comprises six DBL (Duffy-binding-like) domains and a single CIDR (cysteine-rich inter-domain region) domain. The C-terminal domain DBL6$\varepsilon$, the most polymorphic domain of VAR2CSA, has seven regions of high variability termed variable blocks (VBs). Here we have determined the crystal structure of DBL6$\varepsilon$ from the FCR3 parasite line and have compared it with the previously determined structure of that from the 3D7 line. We found significant differences particularly in the N-terminal region, which contains the first VB (VB1). Although DBL6$\varepsilon$ is the most variable VAR2CSA domain, DBL6$\varepsilon$-FCR3 and DBL6$\varepsilon$-3D7 react with IgG purified from immune sera of pregnant women. Furthermore, IgG purified on one domain cross-reacts with the other, confirming the presence of cross-reactive epitopes. We also examined reactivity of immune sera to the four least variable VB (VB1, VB2, VB4 and VB5) using peptides with the consensus sequence closest, in turn, to the FCR3 or 3D7 domain. These results provide new molecular insights into immune escape by parasites expressing the VAR2CSA variant.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=23429057&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2013/Gangnard/J.%20Mol.%20Biol.%202013%20Gangnard.pdf},
file = {{J. Mol. Biol. 2013 Gangnard.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2013/Gangnard/J. Mol. Biol. 2013 Gangnard.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.jmb.2013.02.014}}
}

@article{McVean:2004p455,
author = {McVean, Gilean A T and Myers, Simon R and Hunt, Sarah and Deloukas, Panos and Bentley, David R and Donnelly, Peter},
title = {{The fine-scale structure of recombination rate variation in the human genome}},
year = {2004},
volume = {304},
number = {5670},
pages = {581--584},
month = apr,
affiliation = {Department of Statistics, University of Oxford, Oxford OX1 3TG, UK. mcvean@stats.ox.ac.uk},
doi = {10.1126/science.1092500},
pmid = {15105499},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2011-02-07T06:01:45GMT},
date-modified = {2016-12-23T22:13:13GMT},
abstract = {The nature and scale of recombination rate variation are largely unknown for most species. In humans, pedigree analysis has documented variation at the chromosomal level, and sperm studies have identified specific hotspots in which crossing-over events cluster. To address whether this picture is representative of the genome as a whole, we have developed and validated a method for estimating recombination rates from patterns of genetic variation. From extensive single-nucleotide polymorphism surveys in European and African populations, we find evidence for extreme local rate variation spanning four orders in magnitude, in which 50% of all recombination events take place in less than 10% of the sequence. We demonstrate that recombination hotspots are a ubiquitous feature of the human genome, occurring on average every 200 kilobases or less, but recombination occurs preferentially outside genes.},
url = {http://www.sciencemag.org/content/304/5670/581.long},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2004/McVean/Science%202004%20McVean.pdf},
file = {{Science 2004 McVean.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2004/McVean/Science 2004 McVean.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.1092500}}
}

@article{Mathers:2015dpa,
author = {Mathers, Amy J and Stoesser, Nicole and Sheppard, Anna E and Pankhurst, Louise and Giess, Adam and Yeh, Anthony J and Didelot, Xavier and Turner, Stephen D and Sebra, Robert and Kasarskis, Andrew and Peto, Tim and Crook, Derrick and Sifri, Costi D},
title = {{Klebsiella pneumoniae carbapenemase (KPC) producing K. pneumoniae at a Single Institution: Insights into Endemicity from Whole Genome Sequencing}},
journal = {Antimicrobial agents and chemotherapy},
year = {2015},
volume = {59},
number = {3},
pages = {AAC.04292--14--1663},
month = jan,
publisher = {American Society for Microbiology},
affiliation = {Division of Infectious Diseases and International Health, Department of Medicine, University of Virginia Health System, Charlottesville, Virginia, USA Clinical Microbiology, Department of Pathology, University of Virginia Health System, Charlottesville, Virginia, USA ajm5b@virginia.edu.},
doi = {10.1128/AAC.04292-14},
pmid = {25561339},
pmcid = {PMC4325807},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-12-08T12:17:56GMT},
date-modified = {2018-10-03T11:49:53GMT},
abstract = {The global emergence of Klebsiella pneumoniae carbapenemase-producing K. pneumoniae (KPC-Kp) multilocus sequence type ST258 is widely recognized. Less is known about the molecular and epidemiological details of non-ST258 K. pneumoniae in the setting of an outbreak mediated by an endemic plasmid. We describe the interplay of blaKPC plasmids and K. pneumoniae strains and their relationship to the location of acquisition in a U.S. health care institution. Whole-genome sequencing (WGS) analysis was applied to KPC-Kp clinical isolates collected from a single institution over 5 years following the introduction of blaKPC in August 2007, as well as two plasmid transformants. KPC-Kp from 37 patients yielded 16 distinct sequence types (STs). Two novel conjugative blaKPC plasmids (pKPC_UVA01 and pKPC_UVA02), carried by the hospital index case, accounted for the presence of blaKPC in 21/37 (57%) subsequent cases. Thirteen (35%) isolates represented an emergent lineage, ST941, which contained pKPC_UVA01 in 5/13 (38%) and pKPC_UVA02 in 6/13 (46%) cases. Seven (19%) isolates were the epidemic KPC-Kp strain, ST258, mostly imported from elsewhere and not carrying pKPC_UVA01 or pKPC_UVA02. Using WGS-based analysis of clinical isolates and plasmid transformants, we demonstrate the unexpected dispersal of blaKPC to many non-ST258 lineages in a hospital through spread of at least two novel blaKPC plasmids. In contrast, ST258 KPC-Kp was imported into the institution on numerous occasions, with other blaKPC plasmid vectors and without sustained transmission. Instead, a newly recognized KPC-Kp strain, ST941, became associated with both novel blaKPC plasmids and spread locally, making it a future candidate for clinical persistence and dissemination.},
url = {http://aac.asm.org/lookup/doi/10.1128/AAC.04292-14},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2015/Mathers/Antimicrob.%20Agents%20Chemother.%202015%20Mathers.pdf},
file = {{Antimicrob. Agents Chemother. 2015 Mathers.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2015/Mathers/Antimicrob. Agents Chemother. 2015 Mathers.pdf:application/pdf;Antimicrob. Agents Chemother. 2015 Mathers.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2015/Mathers/Antimicrob. Agents Chemother. 2015 Mathers.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1128/AAC.04292-14}}
}

