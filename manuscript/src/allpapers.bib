@article{Sa:2018iga,
author = {S{\'a}, Juliana M and Kaslow, Sarah R and Krause, Michael A and Melendez-Muniz, Viviana A and Salzman, Rebecca E and Kite, Whitney A and Zhang, Min and Barros, Roberto R Moraes and Mu, Jianbing and Han, Paul K and Mershon, J Patrick and Figan, Christine E and Caleon, Ramoncito L and Rahman, Rifat S and Gibson, Tyler J and Amaratunga, Chanaki and Nishiguchi, Erika P and Breglio, Kimberly F and Engels, Theresa M and Velmurugan, Soundarapandian and Ricklefs, Stacy and Straimer, Judith and Gnadig, Nina F and Deng, Bingbing and Liu, Anna and Diouf, Ababacar and Miura, Kazutoyo and Tullo, Gregory S and Eastman, Richard T and Chakravarty, Sumana and James, Eric R and Udenze, Kenneth and Li, Suzanne and Sturdevant, Daniel E and Gwadz, Robert W and Porcella, Stephen F and Long, Carole A and Fidock, David A and Thomas, Marvin L and Fay, Michael P and Sim, B Kim Lee and Hoffman, Stephen L and Adams, John H and Fairhurst, Rick M and Su, Xin-zhuan and Wellems, Thomas E},
title = {{Artemisinin resistance phenotypes and K13 inheritance in a Plasmodium falciparum cross and Aotus model}},
journal = {Proceedings of the National Academy of Sciences},
year = {2018},
volume = {2},
pages = {201813386},
month = nov,
publisher = {National Academy of Sciences},
affiliation = {Laboratory of Malaria and Vector Research, National Institute of Allergy and Infectious Diseases, National Institutes of Health, Bethesda, MD 20892.},
doi = {10.1073/pnas.1813386115},
pmid = {30455312},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-11-21T16:37:51GMT},
date-modified = {2018-11-28T10:56:32GMT},
abstract = {Artemisinin-based combination therapies (ACTs) are first-line antimalarial therapies used worldwide. The artemisinin drug (ART) component clears the bulk of infection rapidly, but small numbers of persistent parasites must be removed by the partner drug. Longer parasite clearance t 1/2 values have been associated with a Kelch-propeller mutation (K13 C580Y), raising concerns of increased ART resistance. We investigated effects of C580Y by using a Plasmodium falciparum cross and a monkey malaria model. Following three standard doses of ART, infections with or without the C580Y mutation cleared to microscopically undetectable levels, as in humans; however, frequent recrudescences occurred with both types of infection. These results emphasize the importance of effective partner drugs to kill the parasites that persist through the ART component of ACT. Concerns about malaria parasite resistance to treatment with artemisinin drugs (ARTs) have grown with findings of prolonged parasite clearance t 1/2s (>5 h) and their association with mutations in Plasmodium falciparum Kelch-propeller protein K13. Here, we describe a P. falciparum laboratory cross of K13 C580Y mutant with C580 wild-type parasites to investigate ART response phenotypes in vitro and in vivo. After genotyping >400 isolated progeny, we evaluated 20 recombinants in vitro: IC50 measurements of dihydroartemisinin were at similar low nanomolar levels for C580Y- and C580-type progeny (mean ratio, 1.00; 95% CI, 0.62{\textendash}1.61), whereas, in a ring-stage survival assay, the C580Y-type progeny had 19.6-fold (95% CI, 9.76{\textendash}39.2) higher average counts. In splenectomized Aotus monkeys treated with three daily doses of i.v. artesunate, t 1/2 calculations by three different methods yielded mean differences of 0.01 h (95% CI, {\textminus}3.66 to 3.67), 0.80 h (95% CI, {\textminus}0.92 to 2.53), and 2.07 h (95% CI, 0.77{\textendash}3.36) between C580Y and C580 infections. Incidences of recrudescence were 57% in C580Y (4 of 7) versus 70% in C580 (7 of 10) infections ({\textminus}13% difference; 95% CI, {\textminus}58% to 35%). Allelic substitution of C580 in a C580Y-containing progeny clone (76H10) yielded a transformant (76H10C580Rev) that, in an infected monkey, recrudesced regularly 13 times over 500 d. Frequent recrudescences of ART-treated P. falciparum infections occur with or without K13 mutations and emphasize the need for improved partner drugs to effectively eliminate the parasites that persist through the ART component of combination therapy.},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1813386115},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Files/24/24F78A8D-6260-4615-B20B-961DC6223323.pdf},
file = {{24F78A8D-6260-4615-B20B-961DC6223323.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Files/24/24F78A8D-6260-4615-B20B-961DC6223323.pdf:application/pdf;24F78A8D-6260-4615-B20B-961DC6223323.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Files/24/24F78A8D-6260-4615-B20B-961DC6223323.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1813386115}}
}

