title = {{Novel venom gene discovery in the platypus.}},
journal = {Genome Biology},
year = {2010},
volume = {11},
number = {9},
pages = {R95},
affiliation = {Faculty of Veterinary Science, The University of Sydney, Regimental Crescent, Camperdown, NSW 2006, Australia. camilla.whittington@sydney.edu.au},
