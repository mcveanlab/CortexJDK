@article{Itzkovitz:2010bu,
author = {Itzkovitz, Shalev and Hodis, Eran and Segal, Eran},
title = {{Overlapping codes within protein-coding sequences.}},
year = {2010},
volume = {20},
number = {11},
pages = {1582--1589},
month = nov,
affiliation = {Department of Computer Science and Applied Mathematics, Weizmann Institute of Science, Rehovot 76100, Israel},
doi = {10.1101/gr.105072.110},
pmid = {20841429},
pmcid = {PMC2963821},
language = {English},
rating = {0},
date-added = {2011-10-15T19:48:28GMT},
date-modified = {2016-12-23T22:13:18GMT},
abstract = {Genomes encode multiple signals, raising the question of how these different codes are organized along the linear genome sequence. Within protein-coding regions, the redundancy of the genetic code can, in principle, allow for the overlapping encoding of signals in addition to the amino acid sequence, but it is not known to what extent genomes exploit this potential and, if so, for what purpose. Here, we systematically explore whether protein-coding regions accommodate overlapping codes, by comparing the number of occurrences of each possible short sequence within the protein-coding regions of over 700 species from viruses to plants, to the same number in randomizations that preserve amino acid sequence and codon bias. We find that coding regions across all phyla encode additional information, with bacteria carrying more information than eukaryotes. The detailed signals consist of both known and potentially novel codes, including position-dependent secondary RNA structure, bacteria-specific depletion of transcription and translation initiation signals, and eukaryote-specific enrichment of microRNA target sites. Our results suggest that genomes may have evolved to encode extensive overlapping information within protein-coding regions.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20841429&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2010/Itzkovitz/Genome%20Res%202010%20Itzkovitz.pdf},
file = {{Genome Res 2010 Itzkovitz.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2010/Itzkovitz/Genome Res 2010 Itzkovitz.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1101/gr.105072.110}}
}

