file = {{Genome Res 2012 Howald.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2012/Howald/Genome Res 2012 Howald.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1101/gr.134478.111}}
