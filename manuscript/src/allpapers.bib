}

@article{Mackinnon:2010p1519,
author = {Mackinnon, M J and Marsh, K},
title = {{The selection landscape of malaria parasites.}},
year = {2010},
volume = {328},
number = {5980},
pages = {866--871},
month = may,
affiliation = {Kenya Medical Research Institute-Wellcome Trust Research Programme, Post Office Box 230, Kilifi, Kenya. mmackinnon@kilifi.kemri-wellcome.org},
doi = {10.1126/science.1185410},
pmid = {20466925},
language = {English},
rating = {0},
date-added = {2011-03-10T23:27:32GMT},
date-modified = {2016-12-23T22:13:14GMT},
abstract = {Malaria parasites have to survive and transmit within a highly selective and ever-changing host environment. Because immunity to malaria is nonsterilizing and builds up slowly through repeated infections, commonly the parasite invades a host that is immunologically and physiologically different from its previous host. During the course of infection, the parasite must also keep pace with changes in host immune responses and red-blood-cell physiology. Here, we describe the "selection landscape" of the most virulent of the human malaria parasites, Plasmodium falciparum, and the adaptive mechanisms it uses to navigate through that landscape. Taking a cost-benefit view of parasite fitness, we consider the evolutionary outcomes of the most important forces of selection operating on the parasite, namely immunity, host death, drugs, mosquito availability, and coinfection. Given the huge potential for malaria parasite evolution in the context of the recently renewed effort to eradicate malaria, a deeper understanding of P. falciparum adaptation is essential.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20466925&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2010/Mackinnon/Science%202010%20Mackinnon.pdf},
file = {{Science 2010 Mackinnon.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2010/Mackinnon/Science 2010 Mackinnon.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.1185410}}
}

@article{Noordenbos:2012fp,
