year = {2005},
volume = {437},
number = {7062},
pages = {1162--1166},
month = oct,
affiliation = {The Institute for Genomic Research, 9712 Medical Center Dr., Rockville, Maryland 20850, USA.},
doi = {10.1038/nature04239},
pmid = {16208317},
language = {English},
read = {Yes},
