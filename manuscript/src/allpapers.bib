title = {{Modeling gene expression using chromatin features in various cellular contexts.}},
journal = {Genome Biology},
year = {2012},
volume = {13},
number = {9},
pages = {R53},
month = sep,
affiliation = {Program in Bioinformatics and Integrative Biology, Department of Biochemistry and Molecular Pharmacology, University of Massachusetts Medical School, Worcester, MA 01605, USA. zhiping.weng@umassmed.edu.},
doi = {10.1186/gb-2012-13-9-r53},
