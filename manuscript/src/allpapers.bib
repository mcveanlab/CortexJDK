local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Files/2F/2FFC0C74-D565-4157-AA85-82A878C629C4.pdf},
file = {{2FFC0C74-D565-4157-AA85-82A878C629C4.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Files/2F/2FFC0C74-D565-4157-AA85-82A878C629C4.pdf:application/pdf;2FFC0C74-D565-4157-AA85-82A878C629C4.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Files/2F/2FFC0C74-D565-4157-AA85-82A878C629C4.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/bioinformatics/btu284}}
}

