month = jan,
affiliation = {Queensland Statistical Genetics Laboratory, Queensland Institute of Medical Research, 300 Herston Road, Brisbane, Queensland 4006, Australia. jian.yang@qimr.edu.au},
doi = {10.1016/j.ajhg.2010.11.011},
