uri = {\url{papers3://publication/doi/10.1038/nrg2544}}
}

@article{Pantazes:2010ft,
author = {Pantazes, R J and Maranas, C D},
title = {{OptCDR: a general computational method for the design of antibody complementarity determining regions for targeted epitope binding.}},
journal = {Protein engineering, design {\&} selection : PEDS},
year = {2010},
volume = {23},
number = {11},
pages = {849--858},
month = nov,
affiliation = {Department of Chemical Engineering, The Pennsylvania State University, University Park, PA 16802, USA.},
doi = {10.1093/protein/gzq061},
pmid = {20847101},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-11-08T21:23:49GMT},
date-modified = {2013-06-15T23:29:07GMT},
abstract = {Antibodies are an important class of proteins with many biomedical and biotechnical applications. Although there are a plethora of experimental techniques geared toward their efficient production, there is a paucity of computational methods for their de novo design. OptCDR is a general computational method to design the binding portions of antibodies to have high specificity and affinity against any targeted epitope of an antigen. First, combinations of canonical structures for the antibody complementarity determining regions (CDRs) that are most likely to be able to favorably bind the antigen are selected. This is followed by the simultaneous refinement of the CDR structures' backbones and optimal amino acid selection for each position. OptCDR is applied to three computational test cases: a peptide from the capsid of hepatitis C, the hapten fluorescein and the protein vascular endothelial growth factor. The results demonstrate that OptCDR can efficiently generate diverse antibody libraries of a pre-specified size with promising antigen affinity potential as exemplified by computationally derived binding metrics.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20847101&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2010/Pantazes/Protein%20Eng.%20Des.%20Sel.%202010%20Pantazes.pdf},
file = {{Protein Eng. Des. Sel. 2010 Pantazes.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2010/Pantazes/Protein Eng. Des. Sel. 2010 Pantazes.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/protein/gzq061}}
}

@article{Eid:2009p317,
