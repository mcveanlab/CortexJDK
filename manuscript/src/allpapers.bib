@article{Sturm:2006jp,
author = {Sturm, Angelika and Amino, Rogerio and van de Sand, Claudia and Regen, Tommy and Retzlaff, Silke and Rennenberg, Annika and Krueger, Andreas and Pollok, J{\"o}rg-Matthias and M{\'e}nard, Robert and Heussler, Volker T},
