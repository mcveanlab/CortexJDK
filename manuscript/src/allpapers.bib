@article{Fontana:2018eq,
author = {Fontana, Maria Chiara and Marconi, Giovanni and Feenstra, Jelena D Milosevic and Fonzi, Eugenio and Papayannidis, Cristina and di Ror{\'a}, Andrea Ghelli Luserna and Padella, Antonella and Solli, Vincenza and Franchini, Eugenia and Ottaviani, Emanuela and Ferrari, Anna and Baldazzi, Carmen and Testoni, Nicoletta and Iacobucci, Ilaria and Soverini, Simona and Haferlach, Torsten and Guadagnuolo, Viviana and Semerad, Lukas and Doubek, Michael and Steurer, Michael and Racil, Zdenek and Paolini, Stefania and Manfrini, Marco and Cavo, Michele and Simonetti, Giorgia and Kralovics, Robert and Martinelli, Giovanni},
