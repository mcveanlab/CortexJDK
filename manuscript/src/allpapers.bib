month = sep,
affiliation = {Program in Bioinformatics and Integrative Biology and Department of Biochemistry and Molecular Pharmacology, University of Massachusetts Medical School, Worcester, MA 01605, USA. Zhiping.Weng@umassmed.edu.},
doi = {10.1186/gb-2012-13-9-r50},
pmid = {22951020},
language = {English},
read = {Yes},
