title = {{Experiments with the Site Frequency Spectrum}},
journal = {Bulletin of mathematical biology},
year = {2010},
month = dec,
affiliation = {Biomathematics Research Centre, Private Bag 4800, Christchurch, 8041, New Zealand, r.sainudiin@math.canterbury.ac.nz.},
doi = {10.1007/s11538-010-9605-5},
