pages = {1071--1092},
month = feb,
publisher = {Genetics Society of America},
affiliation = {Department of Human Genetics, University of Chicago, Chicago, Illinois 60637, USA. szoellne@genetics.uchicago.edu},
doi = {10.1534/genetics.104.031799},
pmid = {15489534},
