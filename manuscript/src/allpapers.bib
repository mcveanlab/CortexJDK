title = {{DNA sequencing of a cytogenetically normal acute myeloid leukaemia genome}},
year = {2008},
volume = {456},
number = {7218},
pages = {66--72},
month = nov,
affiliation = {Department of Medicine, Washington University School of Medicine, St. Louis, Missouri 63108, USA.},
