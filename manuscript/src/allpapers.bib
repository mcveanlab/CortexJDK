@book{Clark:2008vk,
author = {Clark, William R},
title = {{In Defense of Self: How the Immune System Really Works}},
publisher = {Oxford University Press, USA},
year = {2008},
edition = {1},
month = feb,
isbn = {9780195335552},
language = {English},
read = {Yes},
