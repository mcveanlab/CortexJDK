@article{Peer:2006p226,
author = {Pe'er, Itsik and Chretien, Yves R and de Bakker, Paul I W and Barrett, Jeffrey C and Daly, Mark J and Altshuler, David M},
title = {{Biases and reconciliation in estimates of linkage disequilibrium in the human genome}},
year = {2006},
volume = {78},
number = {4},
