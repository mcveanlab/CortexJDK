file = {{Nature 2011 Locke.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Reports/2011/Locke/Nature 2011 Locke.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/nature09687}}
}

@article{DixonSalazar:2012ee,
