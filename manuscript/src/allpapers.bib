affiliation = {Kyowa Hakko Kogyo, Tokyo Research Laboratories, 3-6-6 Asahimachi, Machidashi, Tokyo 194-8533, Japan.},
doi = {10.1128/JB.186.12.3911-3921.2004},
pmid = {15175305},
