volume = {23},
number = {8},
pages = {1271--1282},
month = aug,
publisher = {Cold Spring Harbor Lab},
affiliation = {Laboratory of Phytopathology, Wageningen University, Droevendaalsesteeg 1, 6708 PB Wageningen, The Netherlands.},
