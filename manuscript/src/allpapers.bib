@article{Daly:2003p189,
author = {Daly, Mark},
title = {{Sugars, insulin sensitivity, and the postprandial state}},
journal = {The American journal of clinical nutrition},
year = {2003},
volume = {78},
number = {4},
pages = {865S--872S},
month = oct,
affiliation = {Diabetes and Vascular Research Centre, Exeter, United Kingdom. m.e.daly@exeter.ac.uk},
pmid = {14522751},
rating = {0},
date-added = {2010-06-13T05:20:34GMT},
date-modified = {2016-12-23T22:13:12GMT},
abstract = {Since insulin resistance was first associated with type 2 diabetes and later with cardiovascular disease and hypertension, there has been considerable interest in the role of dietary and environmental factors. Sucrose and fructose have been a particular research focus. Research on animals, particularly rodents, has shown a clear and consistent effect of high-sucrose and high-fructose diets in decreasing insulin sensitivity. Experiments in humans have produced very conflicting results, with limited evidence for a negative effect on insulin sensitivity at higher intakes of fructose or sucrose (generally > 30% of daily energy from sucrose and > 15% of daily energy from fructose). Observation studies in humans have not shown a link between sucrose consumption and insulin sensitivity independent of other dietary factors. This is in contrast with several small studies that showed an improvement in insulin sensitivity after subjects followed dietary advice to lower the glycemic index of their food choices (where sugars were not a target for change). However, the pattern of postprandial glucose and insulin responses elicited by sucrose and fructose differs substantially from that elicited by starches, with lower troughs elicited by sucrose and fructose 2-3 h after eating. These differences in the pattern of postprandial responses offer a potential explanation for the conflicting results on insulin sensitivity, with the possibility that increases in insulin exposure may affect insulin sensitivity through down-regulation of insulin action.},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed&cmd=Retrieve&dopt=AbstractPlus&list_uids=14522751},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2003/Daly/Am%20J%20Clin%20Nutr%202003%20Daly.pdf},
file = {{Am J Clin Nutr 2003 Daly.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2003/Daly/Am J Clin Nutr 2003 Daly.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/478B9A32-659B-4F3E-AA88-8BA2EF8AB9AB}}
