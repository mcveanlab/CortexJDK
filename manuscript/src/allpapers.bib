journal = {Cell},
year = {2011},
volume = {147},
number = {1},
pages = {185--198},
month = sep,
affiliation = {Cold Spring Harbor Laboratory, Cold Spring Harbor, NY 11724, USA.},
doi = {10.1016/j.cell.2011.08.036},
