month = jan,
affiliation = {Centre for Medical Parasitology at Department of Infectious Diseases, Copenhagen University Hospital (Rigshospitalet) and Institute for Medical Microbiology and Immunology, University of Copenhagen, Copenhagen, Denmark.},
doi = {10.1111/j.1365-2958.2006.05503.x},
pmid = {17176260},
