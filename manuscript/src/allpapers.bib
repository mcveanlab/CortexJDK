@article{Wellems:1990eg,
author = {Wellems, T E and Panton, L J and Gluzman, I Y and do Rosario, V E and Gwadz, R W and Walker-Jonah, A and Krogstad, D J},
title = {{Chloroquine resistance not linked to mdr-like genes in a Plasmodium falciparum cross.}},
journal = {Nature},
year = {1990},
volume = {345},
number = {6272},
pages = {253--255},
month = may,
affiliation = {Laboratory of Parasitic Diseases, National Institute of Allergy, Infectious Diseases, National Institutes of Health, Bethesda, Maryland 20892.},
doi = {10.1038/345253a0},
pmid = {1970614},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-09-08T10:36:55GMT},
date-modified = {2018-10-03T11:49:29GMT},
abstract = {Chloroquine is thought to act against falciparum malaria by accumulating in the acid vesicles of the parasite and interfering with their function. Parasites resistant to chloroquine expel the drug rapidly in an unaltered form, thereby reducing levels of accumulation in the vesicles. The discovery that verapamil partially reverses chloroquine resistance in vitro led to the proposal that efflux may involve an ATP-driven P-glycoprotein pump similar to that in mammalian multidrug-resistant (mdr) tumor cell lines. Indeed, Plasmodium falciparum contains at least two mdr-like genes, one of which has been suggested to confer the chloroquine resistant (CQR) phenotype. To determine if either of these genes is linked to chloroquine resistance, we performed a genetic cross between CQR and chloroquine-susceptible (CQS) clones of P. falciparum. Examination of 16 independent recombinant progeny indicated that the rapid efflux phenotype is controlled by a single gene or a closely linked group of genes. But, there was no linkage between the rapid efflux, CQR phenotype and either of the mdr-like P. falciparum genes or amplification of those genes. These data indicate that the genetic locus governing chloroquine efflux and resistance is independent of the known mdr-like genes.},
url = {http://www.nature.com/doifinder/10.1038/345253a0},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Files/CE/CE5EB5C0-84B3-46A6-A5BA-FC4FDCAF66F3.pdf},
file = {{CE5EB5C0-84B3-46A6-A5BA-FC4FDCAF66F3.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Files/CE/CE5EB5C0-84B3-46A6-A5BA-FC4FDCAF66F3.pdf:application/pdf;CE5EB5C0-84B3-46A6-A5BA-FC4FDCAF66F3.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Files/CE/CE5EB5C0-84B3-46A6-A5BA-FC4FDCAF66F3.pdf:application/pdf}},
