title = {{Inference of homologous recombination in bacteria using whole-genome sequences.}},
journal = {Genetics},
year = {2010},
volume = {186},
number = {4},
pages = {1435--1449},
month = dec,
affiliation = {Department of Statistics, University of Oxford, Oxford, UK. xavier.didelot@gmail.com},
doi = {10.1534/genetics.110.120121},
pmid = {20923983},
