title = {{Strobe sequence design for haplotype assembly.}},
journal = {BMC bioinformatics},
year = {2011},
volume = {12 Suppl 1},
pages = {S24},
affiliation = {Department of Computer Science and Engineering, University of California, San Diego, La Jolla, CA 92093, USA. cylo@cs.ucsd.edu},
doi = {10.1186/1471-2105-12-S1-S24},
