number = {6},
pages = {e1000529},
month = jun,
affiliation = {Department of Statistics, University of Oxford, Oxford, UK.},
doi = {10.1371/journal.pgen.1000529},
pmid = {19543373},
pmcid = {PMC2689936},
language = {English},
read = {Yes},
