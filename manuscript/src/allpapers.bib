file = {{Science 2002 Gabriel.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2002/Gabriel/Science 2002 Gabriel.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.1069424}}
}

