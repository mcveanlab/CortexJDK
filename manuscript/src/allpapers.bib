title = {{Patterns and rates of exonic de novo mutations in autism spectrum disorders.}},
year = {2012},
volume = {485},
number = {7397},
pages = {242--245},
month = may,
affiliation = {Analytic and Translational Genetics Unit, Department of Medicine, Massachusetts General Hospital and Harvard Medical School, Boston, Massachusetts 02114, USA.},
doi = {10.1038/nature11011},
pmid = {22495311},
