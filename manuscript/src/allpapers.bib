journal = {Molecular Biology International},
year = {2014},
volume = {2014},
number = {8},
pages = {1--8},
month = aug,
publisher = {Hindawi Publishing Corporation},
affiliation = {Joint BioEnergy Institute, Emeryville, CA, USA ; Sandia National Laboratories, Livermore, CA, USA.},
doi = {10.1155/2014/287430},
pmid = {25197572},
