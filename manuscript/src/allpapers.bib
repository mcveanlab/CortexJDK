@article{Mu:2005p1407,
author = {Mu, Jianbing and Awadalla, Philip and Duan, Junhui and McGee, Kate M and Joy, Deirdre A and McVean, Gilean A T and Su, Xin-zhuan},
title = {{Recombination hotspots and population structure in Plasmodium falciparum.}},
journal = {PLoS biology},
year = {2005},
volume = {3},
number = {10},
pages = {e335},
month = oct,
affiliation = {Laboratory of Malaria and Vector Research, National Institutes of Health, Rockville, Maryland, USA.},
doi = {10.1371/journal.pbio.0030335},
pmid = {16144426},
pmcid = {PMC1201364},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-03-10T02:23:38GMT},
date-modified = {2016-12-23T22:13:14GMT},
abstract = {Understanding the influences of population structure, selection, and recombination on polymorphism and linkage disequilibrium (LD) is integral to mapping genes contributing to drug resistance or virulence in Plasmodium falciparum. The parasite's short generation time, coupled with a high cross-over rate, can cause rapid LD break-down. However, observations of low genetic variation have led to suggestions of effective clonality: selfing, population admixture, and selection may preserve LD in populations. Indeed, extensive LD surrounding drug-resistant genes has been observed, indicating that recombination and selection play important roles in shaping recent parasite genome evolution. These studies, however, provide only limited information about haplotype variation at local scales. Here we describe the first (to our knowledge) chromosome-wide SNP haplotype and population recombination maps for a global collection of malaria parasites, including the 3D7 isolate, whose genome has been sequenced previously. The parasites are clustered according to continental origin, but alternative groupings were obtained using SNPs at 37 putative transporter genes that are potentially under selection. Geographic isolation and highly variable multiple infection rates are the major factors affecting haplotype structure. Variation in effective recombination rates is high, both among populations and along the chromosome, with recombination hotspots conserved among populations at chromosome ends. This study supports the feasibility of genome-wide association studies in some parasite populations.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16144426&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2005/Mu/PLoS%20Biol%202005%20Mu.pdf},
file = {{PLoS Biol 2005 Mu.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2005/Mu/PLoS Biol 2005 Mu.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pbio.0030335}}
}

@phdthesis{LASTZImprovedPairwiseAlignmentOfDNA2007,
author = {Harris, Robert S},
title = {{Improved Pairwise Alignment of Genomic Dna}},
school = {Pennsylvania State University},
year = {2007},
address = {University Park, PA, USA},
publisher = {Pennsylvania State University},
isbn = {978-0-549-43170-1},
rating = {0},
date-added = {2017-02-24T09:49:01GMT},
date-modified = {2017-02-24T09:49:09GMT},
abstract = {Advances in DNA sequencing technology have fueled a rapid increase in the number of sequenced vertebrate genomes, and we anticipate an explosion in the number of genomes sequenced in the near future. Detecting similarities between genomes is a valuable technique in discovering functional elements, and sequence alignment is the primary tool for discovering similarities. The quality of alignments is affected by several user-specified control parameters. The parameters are so little understood that most users simply use default settings. We seek to change that, to have the program automatically infer appropriate parameter choices from statistics derived automatically from the sequences. We introduce a program, INFERZ, which addresses part of the inference problem, inferring substitution and gap scores according to a mathematically sound model. Further, we explore the usefulness of iterating inferred scores to convergence. We test this process on both simulated and actual genomic data, and show that iteration will converge in general, but found that converged scores were not a consistent improvement. INFERZ has a synergistic relationship with LASTZ, our improved drop-in replacement for the widely used alignment program BLASTZ. INFERZ makes repeated calls to LASTZ to test score sets, and LASTZ provides the user an option to have INFERZ decide what scoring parameters to use. Compared to BLASTZ, LASTZ adds a richer set of seeding strategy choices, supports alignment to probabilistic sequences and reduces memory requirements. Additionally, disciplined software techniques make it a better platform for continued experimentation.},
url = {http://portal.acm.org/citation.cfm?id=1414852},
uri = {\url{papers3://publication/uuid/CCE07F3A-89DE-412F-8FEE-B1DAB23A9486}}
}

