file = {{Science 2009 Drmanac.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2009/Drmanac/Science 2009 Drmanac.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.1181498}}
}

