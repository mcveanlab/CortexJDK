@article{Watson:2012fa,
author = {Watson, C T and Breden, F},
title = {{The immunoglobulin heavy chain locus: genetic variation, missing data, and implications for human disease.}},
journal = {Genes and immunity},
year = {2012},
month = may,
affiliation = {Department of Biological Sciences, Simon Fraser University, Burnaby, BC, Canada.},
doi = {10.1038/gene.2012.12},
pmid = {22551722},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-07-27T11:14:36GMT},
date-modified = {2016-12-23T22:13:39GMT},
abstract = {The immunoglobulin (IG) loci consist of repeated and highly homologous sets of genes of different types, variable (V), diversity (D) and junction (J), that rearrange in developing B cells to produce an individual's highly variable repertoire of expressed antibodies, designed to bind to a vast array of pathogens. This repeated structure makes these loci susceptible to a high frequency of insertion and deletion events through evolutionary time, and also makes them difficult to characterize at the genomic level or assay with high-throughput techniques. Given the central role of antibodies in the adaptive immune system, it is not surprising that early candidate gene approaches showed that germline polymorphisms in these regions correlated with susceptibility to both infectious and autoimmune diseases. However, more recent studies, particularly those using high-throughput genome-wide arrays, have failed to implicate these loci in disease. In this review of the IG heavy chain variable gene cluster (IGHV), we examine how poorly we understand the distribution of haplotype variation in this genomic region, and we argue that this lack of information may mask candidate loci in the IGHV gene cluster as causative factors for infectious and autoimmune diseases.Genes and Immunity advance online publication, 3 May 2012; doi:10.1038/gene.2012.12.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22551722&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2012/Watson/Genes%20Immun%202012%20Watson.pdf},
file = {{Genes Immun 2012 Watson.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2012/Watson/Genes Immun 2012 Watson.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/gene.2012.12}}
}

@article{Song:2011el,
author = {Song, De-Gang and Ye, Qunrui and Poussin, Mathilde and Harms, Gretchen M and Figini, Mariangela and Powell, Daniel J},
title = {{CD27 costimulation augments the survival and anti-tumor activity of redirected human T cells in vivo.}},
journal = {Blood},
year = {2011},
month = nov,
affiliation = {Ovarian Cancer Research Center, Department of Obstetrics and Gynecology, University of Pennsylvania, Philadelphia, PA, United States;},
doi = {10.1182/blood-2011-03-344275},
pmid = {22117050},
language = {English},
rating = {0},
date-added = {2011-12-26T05:53:10GMT},
date-modified = {2013-06-15T23:29:07GMT},
abstract = {The costimulatory effects of CD27 on T lymphocyte effector function and memory formation has been confined to evaluations in mouse models, in vitro human cell culture systems and clinical observations. Here, we tested whether CD27 costimulation actively enhances human T cell function, expansion and survival in vitro and in vivo. Human T cells transduced to express an antigen-specific chimeric antigen receptor (CAR-T) containing an intracellular CD3 zeta (CD3$\zeta$) chain signaling module with the CD27 costimulatory motif in tandem exerted increased antigen-stimulated effector functions in vitro, including cytokine secretion and cytotoxicity, compared to CAR-T with CD3$\zeta$ alone. After antigen stimulation in vitro, CD27 bearing CAR-T cells also proliferated, upregulated Bcl-X(L) protein expression, resisted apoptosis and underwent increased numerical expansion. The greatest impact of CD27 was noted in vivo where transferred CAR-T cells with CD27 demonstrated heightened persistence after infusion, facilitating improved regression of human cancer in a xenogeneic allograft model. This tumor regression was similar to that achieved with CD28 or 4-1BB costimulated CARs, and heightened persistence was similar to 4-1BB, but higher than CD28. Thus, CD27 costimulation enhances expansion, effector function and survival of human CAR-T cells in vitro and augments human T cell persistence and anti-tumor activity in vivo.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22117050&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2011/Song/Blood%202011%20Song.pdf},
file = {{Blood 2011 Song.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2011/Song/Blood 2011 Song.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1182/blood-2011-03-344275}}
}

