author = {{The malERA Consultative Group on Diagnoses and Diagnostics} and {The malERA Consultative Group on Diagnoses and Diagnostics}},
title = {{A Research Agenda for Malaria Eradication: Diagnoses and Diagnostics}},
journal = {PLoS medicine},
year = {2011},
volume = {8},
number = {1},
pages = {e1000396},
month = jan,
doi = {10.1371/journal.pmed.1000396.t001},
pmid = {21311583},
language = {English},
rating = {0},
date-added = {2011-03-18T20:39:15GMT},
date-modified = {2016-12-23T22:13:15GMT},
abstract = {Many of malaria's signs and symptoms are indistinguishable from those of other febrile diseases. Detection of the presence of Plasmodium parasites is essential, therefore, to guide case management. Improved diagnostic tools are required to enable targeted treatment of infected individuals. In addition, field-ready diagnostic tools for mass screening and surveillance that can detect asymptomatic infections of very low parasite densities are needed to monitor transmission reduction and ensure elimination. Antibody-based tests for infection and novel methods based on biomarkers need further development and validation, as do methods for the detection and treatment of Plasmodium vivax. Current rapid diagnostic tests targeting P. vivax are generally less effective than those targeting Plasmodium falciparum. Moreover, because current drugs for radical cure may cause serious side effects in patients with glucose-6-phosphate dehydrogenase (G6PD) deficiency, more information is needed on the distribution of G6PD-deficiency variants as well as tests to identify at-risk individuals. Finally, in an environment of very low or absent malaria transmission, sustaining interest in elimination and maintaining resources will become increasingly important. Thus, research is required into the context in which malaria diagnostic tests are used, into diagnostics for other febrile diseases, and into the integration of these tests into health systems.},
url = {http://dx.plos.org/10.1371/journal.pmed.1000396.t001},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2011/The%20malERA%20Consultative%20Group%20on%20Diagnoses%20and%20Diagnostics/PLoS%20Med.%202011%20The%20malERA%20Consultative%20Group%20on%20Diagnoses%20and%20Diagnostics.pdf},
