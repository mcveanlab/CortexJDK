uri = {\url{papers3://publication/doi/10.1155/2014/287430}}
}

@article{Bolger:2017jv,
author = {Bolger, Anthony M and Denton, Alisandra K and Bolger, Marie E and Usadel, Bjoern},
title = {{LOGAN: A framework for LOssless Graph-based ANalysis of high throughput sequence data}},
journal = {bioRxiv},
year = {2017},
pages = {175976},
month = aug,
publisher = {Cold Spring Harbor Labs Journals},
affiliation = {bioRxiv},
doi = {10.1101/175976},
language = {English},
read = {Yes},
rating = {0},
date-added = {2017-08-22T19:16:30GMT},
date-modified = {2018-10-03T11:50:04GMT},
abstract = {<p>Recent massive growth in the production of sequencing data necessitates matching improvements in bioinformatics tools to effectively utilize it. Existing tools suffer from limitations in both scalability and applicability which are inherent to their underlying algorithms and data structures. We identify the key requirements for the ideal data structure for sequence analyses: it should be informationally lossless, locally updatable, and memory efficient; requirements which are not met by data structures underlying the major assembly strategies Overlap Layout Consensus and De Bruijn Graphs. We therefore propose a new data structure, the LOGAN graph, which is based on a memory efficient Sparse De Bruijn Graph with routing information. Innovations in storing routing information and careful implementation allow sequence datasets for Escherichia coli (4.6Mbp, 117x coverage), Arabidopsis thaliana (135Mbp, 17.5x coverage) and Solanum pennellii (1.2Gbp, 47x coverage) to be loaded into memory on a desktop computer in seconds, minutes, and hours respectively. Memory consumption is competitive with state of the art alternatives, while losslessly representing the reads in an indexed and updatable form. Both Second and Third Generation Sequencing reads are supported. Thus, the LOGAN graph is positioned to be the backbone for major breakthroughs in sequence analysis such as integrated hybrid assembly, assembly of exceptionally large and repetitive genomes, as well as assembly and representation of pan-genomes.</p>},
url = {http://biorxiv.org/lookup/doi/10.1101/175976},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2017/Bolger/bioRxiv%202017%20Bolger.pdf},
file = {{bioRxiv 2017 Bolger.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2017/Bolger/bioRxiv 2017 Bolger.pdf:application/pdf;bioRxiv 2017 Bolger.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2017/Bolger/bioRxiv 2017 Bolger.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1101/175976}}
}

