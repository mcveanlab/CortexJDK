title = {{A genome-wide comparison of recent chimpanzee and human segmental duplications}},
institution = {Howard Hughes Medical Institute, Department of Genome Sciences, University of Washington School of Medicine, 1705 NE Pacific Street, Seattle, Washington 98195, USA.},
year = {2005},
month = sep,
affiliation = {Howard Hughes Medical Institute, Department of Genome Sciences, University of Washington School of Medicine, 1705 NE Pacific Street, Seattle, Washington 98195, USA.},
doi = {10.1038/nature04000},
