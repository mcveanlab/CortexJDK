year = {2002},
volume = {18},
number = {3},
pages = {452--464},
month = mar,
publisher = {Oxford University Press},
affiliation = {Department of Chemistry and Biochemistry, University of California, Los Angeles, Los Angeles, CA 90095-1570, USA. leec@mbi.ucla.edu},
