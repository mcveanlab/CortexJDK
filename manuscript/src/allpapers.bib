volume = {6},
number = {4},
pages = {291--295},
month = mar,
affiliation = {The Wellcome Trust Sanger Institute, Wellcome Trust Genome Campus, Hinxton, Cambridge, UK.},
doi = {10.1038/nmeth.1311},
pmid = {19287394},
