journal = {Vaccine},
year = {2011},
volume = {29},
number = {11},
pages = {2085--2091},
month = mar,
affiliation = {Department of Medicine, Division of Gastroenterology, Rhode Island Hospital {\&} Warren Alpert Medical School of Brown University, Providence, RI 02903, USA. Steven Moss@Brown.edu},
doi = {10.1016/j.vaccine.2010.12.130},
