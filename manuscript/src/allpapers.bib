@article{Pei:2012ji,
author = {Pei, Baikang and Sisu, Cristina and Frankish, Adam and Howald, Cedric and Habegger, Lukas and Mu, Xinmeng Jasmine and Harte, Rachel and Balasubramanian, Suganthi and Tanzer, Andrea and Diekhans, Mark and Reymond, Alexandre and Hubbard, Tim J and Harrow, Jennifer and Gerstein, Mark B},
title = {{The GENCODE pseudogene resource.}},
journal = {Genome Biology},
year = {2012},
volume = {13},
number = {9},
pages = {R51},
month = sep,
affiliation = {Program in Computational Biology and Bioinformatics, Yale University, Bass 432, 266 Whitney Avenue, New Haven, CT 06520, USA. mark.gerstein@yale.edu.},
doi = {10.1186/gb-2012-13-9-r51},
pmid = {22951037},
language = {English},
rating = {0},
date-added = {2012-09-10T12:46:49GMT},
date-modified = {2018-10-03T11:48:58GMT},
abstract = {ABSTRACT: BACKGROUND: Pseudogenes have long been considered as nonfunctional genomic sequences. However, recent evidence suggests that many of them might have some form of biological activity, and the possibility of functionality has increased interest in their accurate annotation and integration with functional genomics data. RESULTS: As part of the GENCODE annotation of the human genome, we present the first genome-wide pseudogene assignment for protein-coding genes, based on both large-scale manual annotation and in silico pipelines. A key aspect of this coupled approach is that it allows us to identify pseudogenes in an unbiased fashion as well as untangle complex events through manual evaluation. We integrate the pseudogene annotations with the extensive ENCODE functional genomics information. In particular, we determine the expression level, transcription-factor and RNA polymerase II binding, and chromatin marks associated with each pseudogene. Based on their distribution, we develop simple statistical models for each type of activity, which we validate with large-scale RT-PCR-Seq experiments. Finally, we compare our pseudogenes with conservation and variation data from primate alignments and the 1000 Genomes project, producing lists of pseudogenes potentially under selection. CONCLUSIONS: At one extreme, some pseudogenes possess conventional characteristics of functionality; these may represent genes that have recently died. On the other hand, we find interesting patterns of partial activity, which may suggest that dead genes are being resurrected as functioning non-coding RNAs. The activity data of each pseudogene are stored in an associated resource, psiDR, which will be useful for the initial identification of potentially functional pseudogenes.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22951037&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2012/Pei/Genome%20Biol%202012%20Pei.pdf},
file = {{Genome Biol 2012 Pei.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2012/Pei/Genome Biol 2012 Pei.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/gb-2012-13-9-r51}}
}

