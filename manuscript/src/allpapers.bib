file = {{2008 Clark.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Books/2008/Clark/2008 Clark.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/06438ADC-2B02-45AD-B33A-B9E6B890E9AF}}
}

