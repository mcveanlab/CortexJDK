pmid = {24040893},
pmcid = {PMC3848682},
read = {Yes},
rating = {0},
date-added = {2017-02-24T09:49:05GMT},
date-modified = {2017-10-03T11:19:20GMT},
abstract = {The de Bruijn graph data structure is widely used in next-generation sequencing (NGS). Many programs, e.g. de novo assemblers, rely on in-memory representation of this graph. However, current techniques for representing the de Bruijn graph of a human genome require a large amount of memory ($\ge$30 GB). We propose a new encoding of the de Bruijn graph, which occupies an order of magnitude less space than current representations. The encoding is based on a Bloom filter, with an additional structure to remove critical false positives. An assembly software implementing this structure, Minia, performed a complete de novo assembly of human genome short reads using 5.7 GB of memory in 23 hours.},
url = {http://dx.doi.org/10.1186/1748-7188-8-22},
uri = {\url{papers3://publication/doi/10.1186/1748-7188-8-22}}
}

