@article{Hodges:2007cw,
author = {Hodges, Ashleigh and Sharrocks, Katherine and Edelmann, Mariola and Baban, Dilair and Moris, Arnaud and Schwartz, Olivier and Drakesmith, Hal and Davies, Kay and Kessler, Benedikt and McMichael, Andrew and Simmons, Alison},
title = {{Activation of the lectin DC-SIGN induces an immature dendritic cell phenotype triggering Rho-GTPase activity required for HIV-1 replication.}},
journal = {Nature Immunology},
year = {2007},
volume = {8},
number = {6},
pages = {569--577},
month = jun,
affiliation = {Medical Research Council Human Immunology Unit, Weatherall Institute of Molecular Medicine, John Radcliffe Hospital, Headington, Oxford OX3 9DS, UK.},
doi = {10.1038/ni1470},
pmid = {17496896},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-11-25T16:23:36GMT},
date-modified = {2016-12-23T22:13:27GMT},
abstract = {DC-SIGN, a C-type lectin expressed on dendritic cells (DCs), can sequester human immunodeficiency virus (HIV) virions in multivesicular bodies. Here, using large-scale gene expression profiling and tyrosine-phosphorylated proteome analyses, we characterized signaling mediated by DC-SIGN after activation by either HIV or a DC-SIGN-specific antibody. Activation of DC-SIGN resulted in downregulation of genes encoding major histocompatibility complex class II, Jagged 1 and interferon-response molecules and upregulation of the gene encoding transcription factor ATF3. Phosphorylated proteome analysis showed that HIV- or antibody-stimulated DC-SIGN signaling was mediated by the Rho guanine nucleotide-exchange factor LARG and led to increased Rho-GTPase activity. Activation of LARG in DCs exposed to HIV was required for the formation of virus-T cell synapses. Thus, HIV sequestration by and stimulation of DC-SIGN helps HIV evade immune responses and spread to cells.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17496896&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2007/Hodges/Nat%20Immunol%202007%20Hodges.pdf},
file = {{Nat Immunol 2007 Hodges.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2007/Hodges/Nat Immunol 2007 Hodges.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/ni1470}}
}

