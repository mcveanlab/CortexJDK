@article{Valouev:2006ev,
author = {Valouev, Anton and Schwartz, David C and Zhou, Shiguo and Waterman, Michael S},
title = {{An algorithm for assembly of ordered restriction maps from single DNA molecules}},
journal = {Proceedings of the National Academy of Sciences},
year = {2006},
volume = {103},
number = {43},
pages = {15770--15775},
month = oct,
publisher = {National Acad Sciences},
affiliation = {Department of Mathematics, University of Southern California, 3620 South Vermont Avenue, KAP 108, Los Angeles, CA 90089-2532, USA. valouev@usc.edu},
doi = {10.1073/pnas.0604040103},
pmid = {17043225},
pmcid = {PMC1635078},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-09-27T10:46:12GMT},
date-modified = {2016-12-23T22:13:50GMT},
abstract = {The restriction mapping of a massive number of individual DNA molecules by optical mapping enables assembly of physical maps spanning mammalian and plant genomes; however, not through computational means permitting completely de novo assembly. Existing algorithms are not practical for genomes larger than lower eukaryotes due to their high time and space complexity. In many ways, sequence assembly parallels map assembly, so that the overlap-layout-consensus strategy, recently shown effective in assembling very large genomes in feasible time, sheds new light on solving map construction issues associated with single molecule substrates. Accordingly, we report an adaptation of this approach as the formal basis for de novo optical map assembly and demonstrate its computational feasibility for assembly of very large genomes. As such, we discuss assembly results for a series of genomes: human, plant, lower eukaryote and bacterial. Unlike sequence assembly, the optical map assembly problem is actually more complex because restriction maps from single molecules are constructed, manifesting errors stemming from: missing cuts, false cuts, and high variance of estimated fragment sizes; chimeric maps resulting from artifactually merged molecules; and true overlap scores that are "in the noise" or "slightly above the noise." We address these problems, fundamental to many single molecule measurements, by an effective error correction method using global overlap information to eliminate spurious overlaps and chimeric maps that are otherwise difficult to identify.},
url = {http://www.pnas.org/content/103/43/15770.full},
local-url = {file://localhost/Users/kiran/Dropbox/Papers/kiran's%20Library/Library.papers3/Articles/2006/Valouev/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%202006%20Valouev.pdf},
file = {{Proceedings of the National Academy of Sciences 2006 Valouev.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2006/Valouev/Proceedings of the National Academy of Sciences 2006 Valouev.pdf:application/pdf;Proceedings of the National Academy of Sciences 2006 Valouev.pdf:/Users/kiran/Dropbox/Papers/kiran's Library/Library.papers3/Articles/2006/Valouev/Proceedings of the National Academy of Sciences 2006 Valouev.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.0604040103}}
}

